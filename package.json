    "mongodb": "^6.10.0",
    "mongoose": "^8.8.0",
    "node-fetch": "^2.7.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "pm2": "^5.4.2",
    "pretty-ms": "^7.0.1",
