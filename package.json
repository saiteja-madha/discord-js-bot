    "lint-staged": "^15.2.10",
    "node": "^23.1.0",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose",
    "multi-purpose-bot"
