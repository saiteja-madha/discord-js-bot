    "@lavaclient/spotify": "^3.1.0",
    "@octokit/rest": "^21.0.2",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.7.7",
    "btoa": "^1.2.1",
    "common-tags": "^1.8.2",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "discord-gamecord": "^4.4.2",
